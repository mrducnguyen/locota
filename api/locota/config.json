{
  "name": "locota",
  "description": "Locomote API middleware version 1 support /airlines, /airports and /search",
  "apiEndpoints": {
    "development" : {
      "baseUrl" : "http://node.locomote.com/code-task",
      "airlines" : "/airlines",
      "airports" : "/airports\\?q=:query",
      "flight_search" : "/flight_search/:airlineCode\\?date=:date&from=:fromAirport&to=:toAirport"
    },
    "stage" : {
      "baseUrl" : "http://node.locomote.com/code-task",
      "airlines" : "/airlines",
      "airports" : "/airports\\?q=:query",
      "flight_search" : "/flight_search/:airlineCode\\?date=:date&from=:fromAirport&to=:toAirport"
    }
  }
}