{{#flights}}
<tr id="flight_{{key}}">
  <td><img src="img/airlines/{{airline.code}}.png" alt="{{airline.name}}" title="{{airline.name}}"></td>
  <td>Flight number: <strong>{{flightNum}}</strong><br><em>{{plan.shortName}}</em></td>
  <td>{{#formatDateTime}}{{start.dateTime}}{{/formatDateTime}} / {{start.timeZone}}</td>
  <td>{{#formatDateTime}}{{finish.dateTime}}{{/formatDateTime}} / {{finish.timeZone}}</td>
  <td>{{#formatDuration}}{{duration}}{{/formatDuration}}</td>
  <td>{{#formatMoney}}{{price}}{{/formatMoney}}</td>
</tr>
{{/flights}}